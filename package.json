  "description": "ummgoban 공통 패키지",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./network": {
      "types": "./dist/network/index.d.ts"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts"
