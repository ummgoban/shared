{
  "name": "@ummgoban/shared",
  "version": "0.0.1-alpha.16",
  "description": "ummgoban 공통 패키지",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.js"
    },
    "./lib": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib/index.esm.js",
      "require": "./dist/lib/index.js"
    },
    "./lib/types": {
      "types": "./dist/lib/types/index.d.ts",
      "import": "./dist/lib/types/index.esm.js",
      "require": "./dist/lib/types/index.js"
    },
    "./lib/utils": {
      "types": "./dist/lib/utils/index.d.ts",
      "import": "./dist/lib/utils/index.esm.js",
      "require": "./dist/lib/utils/index.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.esm.js",
      "require": "./dist/http/index.js"
    },
    "./http/error": {
      "types": "./dist/http/error/index.d.ts",
      "import": "./dist/http/error/index.esm.js",
      "require": "./dist/http/error/index.js"
    },
    "./http/api-client": {
      "types": "./dist/http/api-client/index.d.ts",
      "import": "./dist/http/api-client/index.esm.js",
      "require": "./dist/http/api-client/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "ci": "./.script/ci.sh",
    "build": "rm -rf dist && rollup -c",
    "test": "vitest run",
    "dev": "vite",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test:all": "yarn lint:check && yarn format:check && yarn type-check && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ummgoban/shared.git"
  },
  "author": "ummgoban",
  "license": "MIT",
  "packageManager": "yarn@3.6.4",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "peerDependencies": {
    "axios": ">=1.7.4",
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.14.4",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@vitejs/plugin-react": "^4.5.0",
    "axios": "^1.7.4",
    "dotenv": "^16.4.7",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^4.41.1",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  }
}
